; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Math_Crusade"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://audio_manager.gd"
ResponsiveLayout="*res://responsive_layout.gd"
GameData="*res://game_data.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=400
window/size/window_height_override=1000
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/handheld/orientation=1

[dotnet]

project/assembly_name="Math_Crusade"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdscript2all/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
